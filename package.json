{
  "name": "maps",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3001",
    "start": "vite --port 3001",
    "build": "tsc && vite build && tsx scripts/post-build.ts",
    "fetch:pl:gminy": "tsx scripts/heraldry/pl/fetch-gminy.ts",
    "fetch:pl:gminy-map": "tsx scripts/heraldry/pl/fetch-gminy-map.ts",
    "fetch:pl:miasta": "tsx scripts/heraldry/pl/fetch-miasta.ts",
    "fetch:pl:miasta-map": "tsx scripts/heraldry/pl/fetch-miasta-map.ts",
    "fetch:pl:powiaty": "tsx scripts/heraldry/pl/fetch-powiaty.ts",
    "fetch:pl:powiaty-map": "tsx scripts/heraldry/pl/fetch-powiaty-map.ts",
    "fetch:pl:wojewodztwa": "tsx scripts/heraldry/pl/fetch-wojewodztwa.ts",
    "fetch:pl:wojewodztwa-map": "tsx scripts/heraldry/pl/fetch-wojewodztwa-map.ts",
    "index:pl": "npm run fetch:pl:gminy-map & nodemon --watch scripts/heraldry/pl/utils/get-markers.ts --exec npm run fetch:pl:miasta-map & nodemon --watch scripts/heraldry/pl/utils/get-markers.ts --exec npm run fetch:pl:powiaty-map",
    "watch:pl": "nodemon --watch scripts/heraldry/* --exec npm run index:pl",
    "resize:pl:coats": "tsx scripts/heraldry/pl/resize-images.ts",
    "web:pl": "tsx scripts/heraldry/pl/web-images.ts",
    "fetch:ee:unit": "tsx scripts/heraldry/ee/fetch-unit.ts",
    "fetch:ee:unit-map": "tsx scripts/heraldry/ee/fetch-unit-map.ts",
    "index:ee": "nodemon --watch scripts/heraldry/ee/utils/get-markers.ts --exec npm run fetch:ee:unit-map",
    "resize:ee:coats": "tsx scripts/heraldry/ee/resize-images.ts",
    "web:ee": "tsx scripts/heraldry/ee/web-images.ts",
    "fetch:fi:kunta": "tsx scripts/heraldry/fi/fetch-kunta.ts",
    "fetch:fi:maakunta": "tsx scripts/heraldry/fi/fetch-maakunta.ts",
    "fetch:fi:kunta-map": "tsx scripts/heraldry/fi/fetch-coats-of-kunta.ts",
    "fetch:fi:maakunta-map": "tsx scripts/heraldry/fi/fetch-coats-of-maakunta.ts",
    "resize:fi:coats": "tsx scripts/heraldry/fi/resize-images.ts",
    "web:fi": "tsx scripts/heraldry/fi/web-images.ts",
    "index:fi": "nodemon --watch scripts/heraldry/fi/utils/get-markers.ts --exec npm run fetch:fi:kunta-map",
    "index:fi:2": "nodemon --watch scripts/heraldry/fi/utils/get-markers.ts --exec npm run fetch:fi:maakunta-map",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix"
  },
  "dependencies": {
    "i18next": "^23.12.2",
    "i18next-intervalplural-postprocessor": "^3.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.0.1",
    "react-use-draggable-scroll": "^0.4.7",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/get-image-colors": "^4.0.5",
    "@types/get-pixels": "^3.3.4",
    "@types/nearest-color": "^0.4.1",
    "@types/node": "^22.4.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "chalk": "^5.3.0",
    "clsx": "^2.1.1",
    "colorthief": "^2.4.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "extract-colors": "^4.0.6",
    "get-image-colors": "^4.0.1",
    "get-pixels": "^3.3.3",
    "join-images": "^1.1.5",
    "nearest-color": "^0.4.4",
    "nodemon": "^3.1.4",
    "p-limit": "^6.1.0",
    "postcss": "^8.4.38",
    "react-helmet": "^6.1.0",
    "sass": "^1.77.8",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.10.0",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.16.0",
    "vite": "^5.3.4",
    "wikipedia": "^2.1.2",
    "wouter": "^3.3.1"
  },
  "resolve-path-alias": {
    "alias": {
      "@/*": "src/*",
      "@components/*": "src/components/*",
      "@assets/*": "src/assets/*"
    }
  }
}
